sample input:
5
6 2 7 4 10
sample output:
3

sample input1:
4
1 2 3 4
sample output1:
4

//code

import java.util.*;
class Height{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
{
a[i]=s.nextInt();
}
int count=1;
for(int i=0;i<n;i++)
{
if(a[0]>a[i])
{
count++;
}
}
System.out.println(count);
}
}

